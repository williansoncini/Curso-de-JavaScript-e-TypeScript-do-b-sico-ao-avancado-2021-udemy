<% if (success.length> 0) { %>
    <div class="row">
        <div class="col my-1">
            <% success.forEach(success => { %>
                <div class="alert alert-success"> <%= success %></div>
            <% })%>
        </div>
    </div>
<% } %>

<% if (errors.length> 0) { %>
    <div class="row">
        <div class="col my-1">
            <% errors.forEach(error => { %>
                <div class="alert alert-danger"> <%= error %></div>
            <% })%>
        </div>
    </div>
<% } %>